    "autoupdate": {
        "url": "https://files.pythonhosted.org/$matchPath",
        "hash": {
            "url": "https://pypi.org/project/cookiecutter/",
            "regex": "(?sm)Hashes for $basename.*?$sha256"
        },
        "extract_dir": "cookiecutter-$version"
    }
